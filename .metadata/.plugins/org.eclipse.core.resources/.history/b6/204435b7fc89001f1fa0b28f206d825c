package Stack;

public class stackDemo1 {
	
	private int stack[], MaxSize, tos;
	
	public void create_Stack(int size)
	{
		
		tos=-1; //init tos
		stack = new int[size];  //create stack
		MaxSize=size; //init MaxSize
		
	}
	
	void push(int e)
	{
		
		tos++;
		stack[tos]=e;
		System.out.println("Element "+e" Pushed:");
	}

}
